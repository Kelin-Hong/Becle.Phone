<UserControl x:Class="Panda.Phone.Publisher.Controls.Tile3"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="180" d:DesignWidth="180">
    
    <Grid x:Name="LayoutRoot">


        <Image x:Name="img_Image" Height="100" Width="100" Stretch="Fill" ></Image>
           
       
        <Image x:Name="img_Allready" Height="30" Width="30" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,0,0,0" Source="/Image_Home/choose.png" Opacity="0"></Image>
        <TextBlock x:Name="tbl_Name" Text="Rank Info" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="13,0,0,9"></TextBlock>
    </Grid>
</UserControl>
